{
  "listen_addr": "127.0.0.1:8443",
  "whitelist": [
    "www.google.com",
    "github.com",
    "*.github.io",
    "www.baidu.com"
  ],
  "ebpf": {
    "enabled": true,
    "sockmap": {
      "enabled": true,
      "max_entries": 65536
    },
    "dns_cache": {
      "enabled": true,
      "max_entries": 10000
    },
    "stats": {
      "enabled": true
    }
  },
  "log": {
    "level": "info",
    "output": "both",
    "file_path": "logs/ebpf-test.log",
    "enable_rotation": true,
    "max_size_mb": 10,
    "max_backups": 3,
    "show_timestamp": true,
    "show_module": true,
    "use_color": true
  }
}
