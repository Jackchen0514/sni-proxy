{
  "listen_addr": "0.0.0.0:8443",
  "whitelist": [
    "www.google.com",
    "github.com",
    "*.github.io"
  ],
  "log": {
    "level": "info",
    "output": "file",
    "file_path": "logs/sni-proxy.log",
    "enable_rotation": true,
    "max_size_mb": 50,
    "max_backups": 3,
    "show_timestamp": true,
    "show_module": true,
    "use_color": false
  }
}
